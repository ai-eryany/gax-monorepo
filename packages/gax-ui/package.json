{
    "name": "@gax/ui",
    "private": true,
    "version": "1.0.0",
    "description": "Core UI for the project",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "tsc && vite build --watch",
        "build": "tsc && vite build",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src",
        "format": "prettier src --write",
        "clean": "rm -rf dist",
        "reset": "rm -rf node_modules && rm -rf package-lock.json && rm -rf dist"
    },
    "dependencies": {
        "@gax/utils": "^1.0.0"
    },
    "peerDependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.26.0"
    },
    "devDependencies": {
        "@gax/types": "^1.0.0"
    }
}
