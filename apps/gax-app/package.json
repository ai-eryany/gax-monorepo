{
    "name": "@gax/app",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --port 3001",
        "build": "tsc && vite build",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src",
        "format": "prettier src --write",
        "count": "git ls-files | xargs cat | wc -l",
        "clean": "rm -rf dist",
        "reset": "rm -rf node_modules && rm -rf package-lock.json",
        "preview": "vite preview"
    },
    "dependencies": {
        "@gax/constants": "^1.0.0",
        "@gax/hooks": "^1.0.0",
        "@gax/ui": "^1.0.0",
        "@gax/utils": "^1.0.0",
        "nanoid": "^5.0.7",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-helmet-async": "^2.0.5",
        "react-icons": "^5.3.0",
        "react-if": "^4.1.5",
        "react-router-dom": "^6.26.0",
        "zustand": "^4.5.5"
    },
    "devDependencies": {
        "@gax/types": "^1.0.0",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-legacy": "^5.4.1",
        "@vitejs/plugin-react": "^4.3.1",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.4.41",
        "sass": "^1.78.0",
        "tailwindcss": "^3.4.9",
        "terser": "^5.31.5",
        "typescript": "^5.5.4",
        "vite": "^5.4.0",
        "vite-plugin-sass-dts": "^1.3.25",
        "vite-tsconfig-paths": "^5.0.1"
    }
}
